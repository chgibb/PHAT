{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "removeComments": true,
        "sourceMap": false,
        "listFiles": false,
        "importHelpers": true,
        "jsx": "react",
        "outDir": ".buildCache/debug/src",
        "incremental": true,
        "strict": true,
        "strictPropertyInitialization": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
}