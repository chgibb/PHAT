{
    "compilerOptions" : 
    {
        "module" : "commonjs",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target" : "es6",
        "noEmitOnError": true,
        "noImplicitAny" : true,
        "noImplicitReturns" : true,
        "noImplicitThis": true,
        "removeComments": true,
        "sourceMap" : false,
        "listFiles" : false,
        "importHelpers": true,
        "jsx": "react",
        "outDir": ".buildCache/debug/",
        "incremental": true,
        "strict": true,
        "strictPropertyInitialization": true
    },
    "files":[
        "src/req/renderer/components/table.tsx",
        "src/req/renderer/components/tabContainer.tsx",
        "src/req/renderer/QCRenderer/QCView.tsx"
    ],
    "exclude": 
    [
        "node_modules"
    ],
    
}
