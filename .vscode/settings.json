{
    "git.ignoreLimitWarning": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/phat-linux-x64/**": true,
        "**/phat-win32-x64/**": true,
        "**/src/**/*.js": true,
        "**/not_node_modules": true,
        "**/tests/**": true,
        "**/.buildCache/**": true,
        "**/forDist/**": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/src/**/*.js": true
    },
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        { "language": "typescript", "autoFix": true }
        { "language": "typescriptreact", "autoFix": true }
    ]
}