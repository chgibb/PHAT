{
    "compilerOptions" : 
    {
        "module" : "es2015",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "target" : "es6",
        "noEmitOnError": true,
        "noImplicitAny" : true,
        "noImplicitReturns" : true,
        "noImplicitThis": true,
        "removeComments": true,
        "sourceMap" : false,
        "listFiles" : false,
        "importHelpers": true,
        "jsx": "react"
    },
    "exclude": 
    [
        "node_modules"
    ]
}
